\
set terminal png;\
set output "spline.png";\
set key top left;\
set tics out;\
set xlabel "x";\
set ylabel "y";\
set title "spline";\
plot \
"out.txt" index 1 using 1:3 with line lw 8 title "linear interp integ"\
,"out.txt" index 2 using 1:3 with line lw 5 title "gsl interp integrate"\
,"out.txt" index 3 using 1:3 with line lw 2 title "quad interp integ"\
        ,"out.txt" index 0 using 1:2 with line lw 8 title "The function"\
        ,"out.txt" index 1 using 1:2 with line lw 5 title "linear interp"\
,"out.txt" index 3 using 1:2 with line lw 3 title "quad interp"\
        ,"out.txt" index 2 using 1:2 with line lw 1 title "gsl interp"\
,"out.txt" index 4 using 1:2 with line lw 5 title "quad diff"\

